            <xml-property name="queryText"><![CDATA[select 
	t.clinic_name,
	t.clinic_code,
	c.item_category_code,
	c.item_code,
	c.item_name,
	c.total_amount + c.price_adjustment
from `transaction` t
LEFT JOIN chargeable_services c ON t.visit_id = c.visit_id
where (t.start_time >= STR_TO_DATE(?,'%Y-%m-%d')
AND t.start_time < ADDDATE(STR_TO_DATE(?,'%Y-%m-%d') , INTERVAL 1 DAY))
/*clinicIds*/
;]]></xml-property>
