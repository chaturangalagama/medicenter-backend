            <method name="beforeOpen"><![CDATA[var doctorIds = params["doctorIds"];

if(doctorIds &amp;&amp; doctorIds.length > 0){
	this.queryText = this.queryText.replace("/*doctorIds*/", "AND t.doctor_id in (" + doctorIds + ") ");
}else {
	this.queryText = this.queryText.replace("/*doctorIds*/", " ");
}




var statuses = params["statuses"];

if(statuses &amp;&amp; statuses.length > 0){
	this.queryText = this.queryText.replace("/*statuses*/", "AND t.payment_status in (" + statuses + ") ");
}else {
	this.queryText = this.queryText.replace("/*statuses*/", " ");
}



var clinicIds = params["clinicIds"];

if(clinicIds &amp;&amp; clinicIds != "\"\"" &amp;&amp; clinicIds.length > 0){
	this.queryText = this.queryText.replace("/*clinicIds*/", "AND t.clinic_id in (" + clinicIds + ") ");
}else {
	this.queryText = this.queryText.replace("/*clinicIds*/", " ");
}

var coverageType = params["coverageType"];

if(coverageType &amp;&amp; coverageType != "\"\"" &amp;&amp; coverageType.length > 0){
	this.queryText = this.queryText.replace("/*coverageType*/", "AND visit_attached_medical_coverages.coverage_type in (" + coverageType + ")");
}else {
	this.queryText = this.queryText.replace("/*coverageType*/", " ");
}
]]></method>
