            <xml-property name="queryText"><![CDATA[SELECT
	tdpmmb.*,
	GROUP_CONCAT(chargeable_services.item_name SEPARATOR "\n\n")as charge,
	GROUP_CONCAT(CONCAT_WS(" ", chargeable_services.quantity, chargeable_services.uom, " ")  SEPARATOR "\n\n")as quantity,
	GROUP_CONCAT( concat('$', format(chargeable_services.total_amount + chargeable_services.price_adjustment, 2))  SEPARATOR "\n\n") AS chargeable
FROM


(SELECT
  tdpmm.*,
  SUM(amount)
FROM
  (SELECT tdpm.*,
    GROUP_CONCAT(CONCAT(purpose, "(", DATE_FORMAT(start_date, '%d-%m-%Y'), ")[", number_of_days,"]") SEPARATOR "\n\n") as date_range
  FROM
    (SELECT tdp.*,
    GROUP_CONCAT(diagnosis.icd10term SEPARATOR "\n") as diagnosis  
    FROM
      (select td.*, 
        sum(case when coverage_type = "CHAS" then payable_amount else 0 end) as total_CHAS,
        sum(case when coverage_type = "MEDISAVE" then payable_amount else 0 end) as total_MEDISAVE, 
        total_patient_paid + sum(case when coverage_type = "CHAS" then payable_amount else 0 end + case when coverage_type = "MEDISAVE" then payable_amount else 0 end) as total_amount_settled, 
        sum(case when coverage_type != "CHAS" and coverage_type != "MEDISAVE" then payable_amount else 0 end) as total_medical_coverage_payable
        FROM
        (select 
        	t.visit_id,
        	t.start_time,
        	t.visit_number,
        	t.patient_number,
        	t.patient_name,
        	t.patient_identity_number,
        	t.doctor_name,
        	t.bill_number,
        	t.payment_status,
        	t.patient_address,
        	t.total_bill_amount,
        	t.total_patient_paid,
        	t.total_patient_payable_amount,
        	t.clinic_group,
        	t.clinic_code,
        	GROUP_CONCAT(visit_attached_medical_coverages.coverage_type SEPARATOR "\n") AS financial_class,
      		GROUP_CONCAT(visit_attached_medical_coverages.medical_coverage_code SEPARATOR "\n") AS medical_class,
      		GROUP_CONCAT(visit_attached_medical_coverages.plan_name SEPARATOR "\n") AS coverage_plan
        	
        	
        from `transaction` t
		LEFT JOIN visit_attached_medical_coverages ON t.visit_id = visit_attached_medical_coverages.visit_id


        WHERE (t.start_time >= STR_TO_DATE(?,'%Y-%m-%d') 
        AND t.start_time < ADDDATE(STR_TO_DATE(?,'%Y-%m-%d') , INTERVAL 1 DAY))
        AND visit_attached_medical_coverages.coverage_name IS NOT NULL
        AND CHAR_LENGTH(visit_attached_medical_coverages.coverage_name) > 0
        AND t.total_bill_amount > 0
        /*coverageType*/
        /*doctorIds*/
        /*statuses*/
        /*clinicIds*/
        GROUP BY t.visit_id) as td
      LEFT JOIN company_payables ON td.visit_id = company_payables.visit_id
      GROUP BY td.visit_id) tdp
	LEFT JOIN diagnosis ON tdp.visit_id = diagnosis.visit_id
    GROUP BY tdp.visit_id) as tdpm
  LEFT JOIN medical_certificates ON tdpm.visit_id = medical_certificates.visit_id
  GROUP BY tdpm.visit_id) as tdpmm
LEFT JOIN bill_payment_info ON tdpmm.visit_id = bill_payment_info.visit_id
GROUP BY tdpmm.visit_id) as tdpmmb
LEFT JOIN chargeable_services ON tdpmmb.visit_id = chargeable_services.visit_id
GROUP BY tdpmmb.visit_id
ORDER BY tdpmmb.medical_class ASC, tdpmmb.clinic_code ASC, tdpmmb.start_time ASC

]]></xml-property>
