            <xml-property name="queryText"><![CDATA[select
	t.visit_id,
	DATE_ADD(t.start_time, INTERVAL -1 DAY) as start_time, 
	t.clinic_group,
	t.clinic_code,
	t.clinic_name,
	t.total_bill_amount as amount
from 
	`transaction` t
WHERE(t.start_time >= STR_TO_DATE(?,'%Y-%m-%d')
AND t.start_time < ADDDATE(STR_TO_DATE(?,'%Y-%m-%d') , INTERVAL 1 DAY))
/*clinicIds*/;]]></xml-property>
