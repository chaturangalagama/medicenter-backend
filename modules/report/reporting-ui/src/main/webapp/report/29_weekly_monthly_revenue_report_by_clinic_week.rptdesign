    <method name="initialize"><![CDATA[function checkFirstWeek(startDate){



	var firstDayOfMonth = BirtDateTime.firstDayOfMonth(startDate);
	var daysToEndOfWeek = 7-BirtDateTime.weekDay(firstDayOfMonth,2);
	var endOfWeek = BirtDateTime.addDay(firstDayOfMonth, daysToEndOfWeek);
	var result = ((BirtDateTime.day(firstDayOfMonth) <= BirtDateTime.day(startDate))) &amp;&amp; (BirtDateTime.day(endOfWeek) >= BirtDateTime.day(startDate));
	if(!result){
		java.lang.System.out.println("NOT WEEK 1: " + startDate.toString());
	}
	return result;
}

reportContext.setGlobalVariable("checkFirstWeek", checkFirstWeek);


function checkSecondWeek(startDate){

	var firstDayOfMonth = BirtDateTime.firstDayOfMonth(startDate);
	var daysToEndOfWeek = 7-BirtDateTime.weekDay(firstDayOfMonth,2);
	var startOfWeek2 = BirtDateTime.addDay(firstDayOfMonth, daysToEndOfWeek + 1);
	var endOfWeek2 = BirtDateTime.addDay(firstDayOfMonth, daysToEndOfWeek + 7);
	
	var result = (BirtDateTime.day(startOfWeek2) <= BirtDateTime.day(startDate) &amp;&amp; BirtDateTime.day(startDate) <= BirtDateTime.day(endOfWeek2));
	if(!result){
		java.lang.System.out.println("NOT WEEK 2: " + startDate.toString() + " | " + BirtDateTime.day(startOfWeek2)  + " | " +  BirtDateTime.day(startDate)+ " | " + BirtDateTime.day(endOfWeek2));
	}
	return result;
}

reportContext.setGlobalVariable("checkSecondWeek", checkSecondWeek);


function checkThirdWeek(startDate){

	var firstDayOfMonth = BirtDateTime.firstDayOfMonth(startDate);
	var daysToEndOfWeek = 7-BirtDateTime.weekDay(firstDayOfMonth,2);
	var startOfWeek3 = BirtDateTime.addDay(firstDayOfMonth, daysToEndOfWeek + 8);
	var endOfWeek3 = BirtDateTime.addDay(firstDayOfMonth, daysToEndOfWeek + 14);
	
	var result =  (BirtDateTime.day(startOfWeek3) <= BirtDateTime.day(startDate) &amp;&amp; BirtDateTime.day(startDate) <= BirtDateTime.day(endOfWeek3));
	if(!result){
		java.lang.System.out.println("NOT WEEK 3: " + startDate.toString());
	}
	return result;
}

reportContext.setGlobalVariable("checkThirdWeek", checkThirdWeek);





function checkForthWeek(startDate){

	var firstDayOfMonth = BirtDateTime.firstDayOfMonth(startDate);
	var daysToEndOfWeek = 7-BirtDateTime.weekDay(firstDayOfMonth,2);
	var startOfWeek4 = BirtDateTime.addDay(firstDayOfMonth, daysToEndOfWeek + 15);
	var endOfWeek4 = BirtDateTime.addDay(firstDayOfMonth, daysToEndOfWeek + 21);
	
	var result = (BirtDateTime.day(startOfWeek4) <= BirtDateTime.day(startDate) &amp;&amp; BirtDateTime.day(startDate) <= BirtDateTime.day(endOfWeek4));
	if(!result){
		java.lang.System.out.println("NOT WEEK 4: " + startDate.toString());
	}
	return result;
}

reportContext.setGlobalVariable("checkForthWeek", checkForthWeek);


function checkFifthWeek(startDate){

	var firstDayOfMonth = BirtDateTime.firstDayOfMonth(startDate);
	var daysToEndOfWeek = 7-BirtDateTime.weekDay(firstDayOfMonth,2);
	var startOfWeek5 = BirtDateTime.addDay(firstDayOfMonth, daysToEndOfWeek + 22);
	var endOfWeek5 = BirtDateTime.addDay(firstDayOfMonth, daysToEndOfWeek + 28);
	
	var result = (BirtDateTime.day(startOfWeek5) <= BirtDateTime.day(startDate) &amp;&amp; BirtDateTime.day(startDate) <= BirtDateTime.day(endOfWeek5));
	if(!result){
		java.lang.System.out.println("NOT WEEK 5: " + startDate.toString());
	}
	return result;
}

reportContext.setGlobalVariable("checkForthWeek", checkForthWeek);]]></method>
