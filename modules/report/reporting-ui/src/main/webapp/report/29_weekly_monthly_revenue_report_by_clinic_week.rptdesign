var week = dimension["Week of Year"]["Week Of Year"];

var date = new Date(year, 0, (1 + (week - 1) * 7));

var startWeek = BirtDateTime.addDay(date, 1 -BirtDateTime.weekDay(date, 2));
var endWeek = BirtDateTime.addDay(startWeek, 6);


var startDateParts = params["startDate"].value.split('-');
var startDate = BirtDateTime.date(startDateParts[0],startDateParts[1]-1,startDateParts[2]);
if(startWeek.getTime() &lt; startDate.getTime()){
	startWeek = startDate;
}


var endDateParts = params["endDate"].value.split('-');
var endDate = BirtDateTime.date(endDateParts[0],endDateParts[1]-1,endDateParts[2]);
if(endWeek.getTime() > endDate.getTime()){
	endWeek = endDate;
}



Formatter.format(startWeek, "dd/MM/yyyy") + "-" + Formatter.format(endWeek, "dd/MM/yyyy");</expression>
