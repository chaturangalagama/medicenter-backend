            <xml-property name="queryText"><![CDATA[SELECT
	t.start_time,
	t.patient_number,
	t.patient_name,
	chargeable_services.item_code,
	chargeable_services.total_amount + chargeable_services.price_adjustment as charges
FROM
	`transaction` t, chargeable_services
WHERE
	t.visit_id = chargeable_services.visit_id
	AND (t.start_time >= STR_TO_DATE(?,'%Y-%m-%d') 
	AND t.start_time < ADDDATE(STR_TO_DATE(?,'%Y-%m-%d') , INTERVAL 1 DAY))
	AND chargeable_services.item_type="MEDICAL_TEST"
	/*clinicIds*/]]></xml-property>
